@model List<NBA_ManagementSystem.Models.Player>

<table class="table table-striped">
    <tr>
        <th>Name</th>
        <th>Position</th>
        <th>Height</th>
        <th>Date of Birth</th>
    </tr>
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.Name</td>
            <td>@p.Position</td>
            <td>@p.Height</td>
            <td>@p.DateOfBirth.ToShortDateString()</td>
        </tr>
    }
</table>
